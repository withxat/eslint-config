{
  "name": "@xats/eslint-config",
  "type": "module",
  "version": "5.0.2",
  "packageManager": "pnpm@10.7.0",
	"description": "🧃 Xat's ESLint config preset / Xat 自用强迫症 ESLint 配置",
	"author": "Xat <i@xat.sh> (https://xat.sh)",
	"license": "MIT",
	"homepage": "https://github.com/withxat/eslint-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/withxat/eslint-config.git"
	},
	"bugs": {
		"url": "https://github.com/withxat/eslint-config/issues"
	},
	"keywords": [
		"eslint-config"
	],
	"exports": {
		".": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
  "scripts": {
    "build": "nr gen && tsdown --clean --dts",
    "stub": "tsdown",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "watch": "tsdown --watch",
    "lint": "eslint",
    "gen": "tsx scripts/typegen.ts && tsx scripts/versiongen.ts",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  	"peerDependencies": {
		"eslint": "^9.5.0"
	},
  "dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
		"@eslint-react/eslint-plugin": "^1.40.1",
		"@eslint/markdown": "^6.3.0",
		"@limegrass/eslint-plugin-import-alias": "^1.5.0",
		"@prettier/plugin-xml": "^3.4.1",
		"@stylistic/eslint-plugin": "^4.2.0",
		"@typescript-eslint/eslint-plugin": "^8.29.0",
		"@typescript-eslint/parser": "^8.29.0",
		"@unocss/eslint-plugin": "66.1.0-beta.8",
		"@vitest/eslint-plugin": "^1.1.38",
		"astro-eslint-parser": "^1.2.2",
		"eslint-config-flat-gitignore": "^2.1.0",
		"eslint-flat-config-utils": "^2.0.1",
		"eslint-merge-processors": "^2.0.0",
		"eslint-plugin-antfu": "^3.1.1",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-command": "^3.2.0",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-import-x": "^4.10.0",
		"eslint-plugin-jsdoc": "^50.6.9",
		"eslint-plugin-jsonc": "^2.20.0",
		"eslint-plugin-n": "^17.17.0",
		"eslint-plugin-no-only-tests": "^3.3.0",
		"eslint-plugin-perfectionist": "^4.11.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"eslint-plugin-regexp": "^2.7.0",
		"eslint-plugin-toml": "^0.12.0",
		"eslint-plugin-unicorn": "^58.0.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"eslint-plugin-vue": "^10.0.0",
		"eslint-plugin-vuejs-accessibility": "^2.4.1",
		"eslint-plugin-yml": "^1.17.0",
		"eslint-processor-vue-blocks": "^2.0.0",
		"globals": "^16.0.0",
		"jsonc-eslint-parser": "^2.4.0",
		"local-pkg": "^1.1.1",
		"parse-gitignore": "^2.0.0",
		"toml-eslint-parser": "^0.10.0",
		"vue-eslint-parser": "^10.1.1",
		"yaml-eslint-parser": "^1.3.0"
	},
  "devDependencies": {
		"@antfu/ni": "^24.3.0",
		"@eslint/config-inspector": "^1.0.2",
		"@types/node": "^22.13.17",
		"@xats/eslint-config": "workspace:*",
		"astro": "^5.5.6",
		"bumpp": "^10.1.0",
		"eslint-typegen": "^2.1.0",
		"lefthook": "^1.11.6",
		"react": "^19.1.0",
		"tsdown": "^0.11.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2",
		"unocss": "66.1.0-beta.8",
		"vue": "^3.5.13"
	}
}
