{
	"name": "@withxat/eslint-config",
	"type": "module",
	"version": "1.0.0",
	"packageManager": "pnpm@10.20.0",
	"description": "🧃 Xat's ESLint config preset / Xat 自用强迫症 ESLint 配置",
	"author": "Xat <i@xat.sh> (https://xat.sh)",
	"license": "MIT",
	"homepage": "https://github.com/withxat/eslint-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/withxat/eslint-config.git"
	},
	"bugs": {
		"url": "https://github.com/withxat/eslint-config/issues"
	},
	"keywords": [
		"eslint",
		"eslint-config"
	],
	"exports": {
		".": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "nr gen && tsdown --clean --dts",
		"stub": "tsdown",
		"dev": "npx @eslint/config-inspector --config eslint.config.ts",
		"build:inspector": "pnpm build && npx @eslint/config-inspector build",
		"watch": "tsdown --watch",
		"lint": "eslint",
		"gen": "tsx scripts/typegen.ts",
		"prepack": "nr build",
		"release": "bumpp && pnpm publish",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"peerDependencies": {
		"eslint": ">=9.29.0"
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
		"@eslint-react/eslint-plugin": "^2.2.4",
		"@eslint/markdown": "^7.5.0",
		"@limegrass/eslint-plugin-import-alias": "^1.6.0",
		"@next/eslint-plugin-next": "^16.0.1",
		"@prettier/plugin-xml": "^3.4.2",
		"@stylistic/eslint-plugin": "^5.5.0",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"@vitest/eslint-plugin": "^1.3.26",
		"astro-eslint-parser": "^1.2.2",
		"eslint-config-flat-gitignore": "^2.1.0",
		"eslint-flat-config-utils": "^2.1.4",
		"eslint-merge-processors": "^2.0.0",
		"eslint-plugin-antfu": "^3.1.1",
		"eslint-plugin-astro": "^1.4.0",
		"eslint-plugin-command": "^3.3.1",
		"eslint-plugin-format": "^1.0.2",
		"eslint-plugin-import-lite": "^0.3.0",
		"eslint-plugin-jsdoc": "^61.1.11",
		"eslint-plugin-jsonc": "^2.21.0",
		"eslint-plugin-n": "^17.23.1",
		"eslint-plugin-no-only-tests": "^3.3.0",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.24",
		"eslint-plugin-regexp": "^2.10.0",
		"eslint-plugin-toml": "^0.12.0",
		"eslint-plugin-unicorn": "^62.0.0",
		"eslint-plugin-unused-imports": "^4.3.0",
		"eslint-plugin-yml": "^1.19.0",
		"globals": "^16.4.0",
		"jsonc-eslint-parser": "^2.4.1",
		"local-pkg": "^1.1.2",
		"parse-gitignore": "^2.0.0",
		"toml-eslint-parser": "^0.10.0",
		"yaml-eslint-parser": "^1.3.0"
	},
	"devDependencies": {
		"@antfu/ni": "^27.0.1",
		"@eslint/config-inspector": "^1.3.0",
		"@types/node": "^24.9.2",
		"@withxat/eslint-config": "workspace:*",
		"@withxat/tsconfig": "^1.0.0",
		"astro": "^5.15.2",
		"bumpp": "^10.3.1",
		"eslint": "^9.38.0",
		"eslint-typegen": "^2.3.0",
		"lefthook": "^2.0.2",
		"react": "^19.2.0",
		"tsdown": "^0.15.12",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
	}
}
